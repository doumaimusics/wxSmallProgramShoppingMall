page{
  background: #f6f6f6;
}
.main_list{
  width:690rpx;
  padding: 0 30rpx;
  margin-bottom:14rpx;
  background:#fff;
}
.stroe_id_name{
  height:84rpx;
  line-height: 84rpx;
}
.stroe_name{
  font-size:26rpx;
  width:530rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.text_stroe{
  font-weight: bold;
}
.stroe_name image{
  width:28rpx;
  height:26rpx;
  position: relative;
  top:2rpx;
  margin-right:6rpx;
}
.stroe_distance{
  font-size:24rpx;
}
.stroe_distance image{
  width:20rpx;
  height:30rpx;
  position: relative;
  top:4rpx;
  margin-right:6rpx;
}
.grab_volume_main{
  display: flex;
  justify-content:space-between;
  align-items: flex-end;
  padding: 26rpx 0 20rpx 0;
  font-size: 28rpx;
}
.address{
  flex: 2;
}
.scanning-body{
  flex:1;
  text-align: center;
  padding-bottom: 2rpx;
}
.scanning-foot{
  flex:1;
  text-align: right;
  padding-bottom: 2rpx;
}
.ciay-image{
  width: 100%;
  height: 100rpx; 
  display: block; 
}
.scanning-image{
  width: 100%;
  height: 100rpx; 
  display: block; 
  margin-left: 22rpx;
}

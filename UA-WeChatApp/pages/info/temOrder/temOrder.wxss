.radio-group{
  padding-left: 125rpx;
  padding-bottom: 0;
  display: none;
  transition: all 0.6s ease;
}
.radio-group.active{
  display: block;
  opacity: 1;
  padding-bottom: 50rpx;
  box-sizing: border-box;
}
.radio-group .radio{
  height:40px;
  display:block;
  line-height:40px;
}
.radio-group radio{
  display: inline-block;
}
.freight .selectUseableCoupon{
  transition: all 0.5s ease;
  transform: rotate(0);
}
.freight .selectUseableCoupon.active{
  transform: rotate(-90deg);
}


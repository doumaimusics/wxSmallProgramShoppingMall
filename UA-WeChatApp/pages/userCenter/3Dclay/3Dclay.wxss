}
.btn-sty{
  position: absolute;
  display: flex;
  width: 100%;
  height: 200rpx;
  padding: 0 50rpx;
  box-sizing: border-box;
  z-index: 200;
}
.default{
  flex: 1;
  color: #ffffff;
  font-size: 34rpx;
  font-weight: bold;
  text-align: center;
  line-height: 200rpx;
}
.actity{
  border-radius: 100rpx;
  padding: 15rpx 30rpx;
  background: #ffffff;
  color: #1476A7;
}
.clay-image{

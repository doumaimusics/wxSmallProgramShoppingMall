.clay-wrapper{
  width: 100%;
  background-image: url('https://wx.3dculab.com/uaxcxv1.0.1/Content/3dBackg.jpg');
  background-repeat:no-repeat; 
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}
.img-wrapper{
  width: 100%;
  height: auto;
}
.img-sty{
  width: 100%;

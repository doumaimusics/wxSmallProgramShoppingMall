.tab-nav-01 {
    width: 750rpx;
    height: 100rpx;
    border-bottom: 2rpx solid #bebebe;
    color: #666;
    display: flex;
    box-sizing: border-box;
    justify-content: space-around;
    font-size: 32rpx;
    line-height: 100rpx;
    background: #fff;
    position: fixed;
    top:0;
    z-index: 99999;
}
.tab-nav-01 .tab-item {
    position: relative;
    height: 100rpx;
    line-height:100rpx;
}
.img_center{
  width:100%;
  background:#fff;
  position: absolute;
  top:0;
}
.img_center image{
    width:100%;
    height:350rpx;
    position: relative;
    top: 34%;
}
.tab-nav-01 .tab-item.cur {
    color: red;
}
.tab-nav-01 .tab-item.cur::after {
    position: absolute;
    left: 0;
    bottom: 0;
    z-index: 999999;
    content: "";
    display: inline-block;
    width: 100%;
    height: 4rpx;
    background-color: red;
}
.filterate-list {
    display: none;
    width: 750rpx;
}
.filterate-list.active {
    display: block;
}
.filterate-item{
  background:#fff;
}
.list_box{
  margin-top:20rpx;
